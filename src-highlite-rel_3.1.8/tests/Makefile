	$(DIFF) $(srcdir)/test.java.texinfo test.java.out.texinfo
	$(DIFF) $(srcdir)/test.java.fodt test.java.out.fodt
	$(DIFF) $(srcdir)/test.java.doc.fodt test.java.out.doc.fodt
