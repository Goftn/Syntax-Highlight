	$(DIFF) $(srcdir)/test_java_lines.html test_java_lines.out.html
	$(DIFF) $(srcdir)/test_no_line_at_the_end.java.html test_no_line_at_the_end.java.out.html
	$(DIFF) $(srcdir)/test.java.txt test.java.out.txt
